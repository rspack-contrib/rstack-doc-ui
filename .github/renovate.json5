{
  $schema: 'https://docs.renovatebot.com/renovate-schema.json',
  extends: ['config:base', 'schedule:monthly', 'group:allNonMajor'],
  rangeStrategy: 'bump',
  packageRules: [
    {
      matchDepTypes: ['peerDependencies'],
      enabled: false,
    },
    {
      matchUpdateTypes: ['patch', 'minor'],
      automerge: true,
      automergeType: 'pr',
      requiredStatusChecks: null,
    },
  ],
}
